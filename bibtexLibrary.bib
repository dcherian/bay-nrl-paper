@article{Yuan2019,
  title = {Dynamics of {{Mesoscale Eddies Interacting}} with a {{Western Boundary Current Flowing}} by a {{Gap}}},
  issn = {2169-9275, 2169-9291},
  abstract = {The propagation of cyclonic and anticyclonic mesoscale eddies through a wide meridional gap with a crossing western boundary current (WBC) is studied using a 1.5‐layer quasi‐geostrophic ocean circulation model. The results of the numerical experiments suggest that a steady, leaping WBC blocks nearly completely all eddies from propagating westward through the gap. In comparison, both cyclonic and anticyclonic eddies propagate westward nearly uninterrupted through the gap if the WBC is in a penetrating state. Interactions of mesoscale eddies with a critical‐state WBC trigger regime transitions of the WBC between leaping and penetrating states, resulting in dramatic changes of the circulation west of the gap. The radiation of the circulation plumes from the gap into the western basin due to the WBC regime transition is found to be independent of the size and the initial location of the perturbing eddy in the eastern basin, provided that the WBC path shift is induced successfully by the eddy. Plain Language Summary The Paciﬁc Ocean exchanges mass and heat with the marginal seas and with the Indian Ocean through the large gaps in its western boundary like the Luzon Strait and the Indonesian seas. This study uses a simple computer model to investigate the nonlinear processes of the water exchange through an idealized barrier gap. The results suggest that when a western boundary current (WBC) ﬂows by the gap along the eastern side of the barrier, the westward movement of mesoscale eddies (ocean swirls at the scales of around 100 km in diameter) through the gap is subject to nonlinear interactions with the WBC. Sometimes, the eddies can induce changes of the WBC path so that opposite anomalies to the approaching eddies are generated in the western basin. The nonlinear theory and the results are important for the understanding of the heat and mass budget of the western Paciﬁc Ocean and of the marginal seas environmental changes, which would otherwise be difﬁcult to explain using the linear theory.},
  language = {en},
  journal = {J. Geophys. Res. Oceans},
  doi = {10/gf4br7},
  author = {Yuan, Dongliang and Song, Xinxin and Yang, Ya and Dewar, William K.},
  month = may,
  year = {2019},
  file = {/home/deepak/Papers/Yuan-et-al-2019-Dynamics_of_Mesoscale_Eddies_Interacting_with_a_Western_Boundary_Current.pdf}
}

