  author = {Gargett, Ann and Garner, Teresa},
  month = sep,
  year = {2008},
  pages = {1657-1670},
  file = {/home/deepak/Papers/Gargett-Garner-2008-Determining_Thorpe_Scales_from_Ship-Lowered_CTD_Density_Profiles.pdf}
}

