@article{Levine2002,
  title = {A {{Modification}} of the {{Garrett}}–{{Munk Internal Wave Spectrum}}},
  volume = {32},
  issn = {0022-3670, 1520-0485},
  abstract = {The Garrett–Munk (GM) spectrum continues to be a useful description of the oceanic internal wave ﬁeld. However, there are several inconsistencies and ambiguities that make it difﬁcult to use in comparing internal wave ﬁelds at different latitudes, stratiﬁcations, and water depths. A modiﬁed spectral formulation is presented that treats three problems with the Garrett–Munk formulation: the normalization of the energy spectrum as a function of frequency bandwidth, the energy distribution at frequencies below the semidiurnal tide, and the treatment of vertical boundaries and turning points. Addressing these problems leads to a substitution of the GM parameters E (nondimensional energy), b (vertical length scale), and N0 (buoyancy frequency scale) with two new dimensional scales: Eref, the energy per unit mass, and D(␻), the Wentzel–Kramers–Brillouin (WKB)scaled thickness of the vertical waveguide. The advantages of the modiﬁed spectrum are illustrated by comparing with observations from the equator and the continental shelf.},
  language = {en},
  number = {11},
  journal = {J. Phys. Oceanogr.},
  doi = {10.1175/1520-0485(2002)032<3166:AMOTGM>2.0.CO;2},
