@article{Shchepetkin,
  title = {A {{Family}} of {{Finite}}-{{Volume Methods}} for {{Computing Pressure Gradient Force}} in an {{Ocean Model}} with a {{Topography}}-{{Following Vertical Coordinate}}},
  abstract = {Discretization of the pressure gradient force is a long-standing problem in σ-coordinate ocean modeling. As long as the isosurfaces of vertical coordinate are not aligned with either geopotential surfaces or isopycnals, the horizontal pressure gradient consists of two large terms that tend to cancel each other. The associated pressure gradient error stems from interference of the discretization errors of these terms. The situation is further complicated by the non-orthogonality of the terrain-following coordinate system and by the common practice of using highly non-uniform stretching for the vertical grids, which, unless special precautions are made, causes both loss of accuracy of vertical discretization, and increase in interference in the error of the pressure gradient force.},
